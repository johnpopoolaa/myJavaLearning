<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">

	<servlet>
		<servlet-name>abc</servlet-name>
		<servlet-class>com.acmetech.AddServlet4</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>abc</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>pqr</servlet-name>
		<servlet-class>com.acmetech.SqServlet4</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>pqr</servlet-name>
		<url-pattern>/sq</url-pattern>
	</servlet-mapping>
</web-app>

<!-- Note that you have to update the servlet class of SqServlet and AddServlet to work with requestDispatcher, 
sendRedirect(URL Rewriting, HttpSession) -->